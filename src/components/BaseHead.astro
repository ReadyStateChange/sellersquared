---
import "../styles/global.css";
import  { AstroSeo }  from "@astrolib/seo";
---

<AstroSeo
  title="SellerSquared - Square Seller tips, tricks and tools to help you get more out of Square!"
  description="Where Square Sellers level up with tips, tricks and tools to help their businesses grow!"
  canonical="https://sellersquared.com"
  openGraph={{
    url: "https://sellersquared.com",
    title: "SellerSquared - Square Seller tips, tricks and tools to help you get more out of Square!",
    description: "Where Square Sellers level up with tips, tricks and tools to help their businesses grow!",
    images: [
      {
        url: "https://sellersquared.com/assets/images/SellerSquared.jpg",
        width: 1200,
        height: 630,
        alt: "SellerSquared - Square Seller tips, tricks and tools to help you get more out of Square!",
        type: "image/jpeg",
      },
    ],
    site_name: "SellerSquared",
    type: "website",
    locale: "en_US",
  }}
  twitter={{
    handle: "@squaretips",
    site: "https://x.com/squaretips",
    cardType: "summary_large_image",
  }}
/>

<meta charset="UTF-8" />
<meta name="viewport" content="width=device-width, initial-scale=1.0" />

<!-- Basic Meta Tags -->
<meta
  name="description"
  content="Get laser focused hacks and tips to make Square work for you"
/>
<meta
  name="keywords"
  content="Square, Hacks, Tips, Square Sellers, Square Online"
/>
<meta name="author" content="SellerSquared" />

<!-- Favicons -->
<link rel="icon" href="/favicons/favicon.ico" sizes="any" />
<link rel="apple-touch-icon" href="/favicons/apple-touch-icon.png" />
<link rel="manifest" href="/manifest.webmanifest" />

<!-- Favicon for IE -->
<link rel="shortcut icon" href="/favicons/favicon.ico" type="image/x-icon" />

<!-- Favicons for different sizes -->
<link
  rel="icon"
  type="image/png"
  sizes="16x16"
  href="/favicons/favicon-16x16.png"
/>
<link
  rel="icon"
  type="image/png"
  sizes="32x32"
  href="/favicons/favicon-32x32.png"
/>
<link
  rel="icon"
  type="image/png"
  sizes="48x48"
  href="/favicons/favicon-48x48.png"
/>

<!-- Open Graph / Facebook -->
<meta property="og:type" content="website" />
<meta property="og:url" content="https://sellersquared.com" />
<meta
  property="og:title"
  content="SellerSquared - Square Hacks and Tips for Square Sellers"
/>
<meta
  property="og:description"
  content="Get laser focused hacks and tips to make Square work for you"
/>
<meta property="og:image" content="https://sellersquared.com/og-image.jpg" />

<!-- Twitter -->
<meta property="twitter:card" content="summary_large_image" />
<meta property="twitter:url" content="https://sellersquared.com" />
<meta
  property="twitter:title"
  content="SellerSquared - Square Hacks and Tips for Square Sellers"
/>
<meta
  property="twitter:description"
  content="Get laser focused hacks and tips to make Square work for you"
/>
<meta
  property="twitter:image"
  content="https://sellersquared.com/og-image.jpg"
/>

<!-- Canonical URL -->
<link rel="canonical" href="https://sellersquared.com" />

<!-- Additional SEO -->
<meta name="robots" content="index, follow" />
<meta name="googlebot" content="index, follow" />

<!-- Apple Touch Icon (already included in favicons, but keeping for backwards compatibility) -->
<link
  rel="apple-touch-icon"
  sizes="180x180"
  href="/favicons/apple-touch-icon.png"
/>

<!-- Theme Color for Mobile Browsers -->
<meta name="theme-color" content="#ffffff" />

<!-- For IE -->
<meta http-equiv="X-UA-Compatible" content="IE=edge" />
<!-- HTML in your document's head -->
<link rel="preconnect" href="https://rsms.me/" />
<link rel="stylesheet" href="https://rsms.me/inter/inter.css" />
<link
  href="https://api.fontshare.com/v2/css?f[]=jet-brains-mono@1,2&display=swap"
  rel="stylesheet"
/>
<link rel="preconnect" href="https://fonts.googleapis.com" />
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
<link
  href="https://fonts.googleapis.com/css2?family=Gilda+Display&display=swap"
  rel="stylesheet"
/>
<script defer src="https://unpkg.com/alpinejs@3.x.x/dist/cdn.min.js"></script>
